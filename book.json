{
  "title": "奶茶店",
  "author": "yinshengphy",
  "description": "二狗的随笔网站，记录笔记，随笔等",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "styles": {
    "website": "static/styles/website.css"
  },
  "structure": {
    "readme": "README.md"
  },
  "plugins": [
    "sharing",
    "github",
    "github-buttons",
    "favicon"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/yinshengphy"
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "yinshengphy",
          "repo": "notes",
          "type": "watch",
          "size": "small",
          "count": true
        }
      ]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "favicon": {
      "shortcut": "static/images/favicon.ico",
      "bookmark": "static/images/favicon.ico",
      "appleTouch": "static/images/favicon.ico",
      "appleTouchMore": {
        "120x120": "static/images/favicon.ico",
        "180x180": "static/images/favicon.ico"
      }
    }
  }
}